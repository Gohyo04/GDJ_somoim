<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <!-- mapper namespace는 비우면 에러 -->
  <mapper namespace="com.somoim.board.notice.NoticeDAO">
  
  <insert id="setAdd" parameterType="BoardDTO">
  	<selectKey keyProperty="noticeNum" resultType="Long" order="BEFORE">
			SELECT notice_seq.NEXTVAL FROM DUAL
	</selectKey>
  	INSERT INTO NOTICEBOARD  
	VALUES(
	#{noticeNum}, #{userName}, #{noticeTitle}, #{noticeContents}, #{noticeWriter}, SYSDATE
	)
  </insert>
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  
  </mapper>